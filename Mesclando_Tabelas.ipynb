    "import pandas as pd\n",
    "tabela_vendas = pd.read_excel('Vendas.xlsx')\n",
    "tabela_gerentes = pd.read_excel('Gerentes.xlsx')\n",
    "#Mesclar Tabelas Vendas + Gerentes\n",
    "tabela_vendas = tabela_vendas.merge(tabela_gerentes)\n",
    "display(tabela_vendas)"
